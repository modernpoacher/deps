{
  "name": "@modernpoacher/deps",
  "version": "1.6.24",
  "description": "Update NPM package dependencies from the command line",
  "keywords": [
    "deps",
    "dependencies",
    "devDependencies",
    "optionalDependencies",
    "bundleDependencies",
    "peerDependencies"
  ],
  "main": "./index.js",
  "author": "Jonathan Perry for Modern Poacher Limited <modernpoacher@modernpoacher.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/modernpoacher/deps"
  },
  "homepage": "https://github.com/modernpoacher/deps",
  "bugs": {
    "url": "https://github.com/modernpoacher/deps/issues"
  },
  "scripts": {
    "prepublishOnly": "cross-env DEBUG_COLORS=yes NODE_ENV=production npm run build",
    "prebuild": "npm run clean",
    "start": "cross-env DEBUG_COLORS=yes NODE_ENV=production node bin/app",
    "dev": "cross-env DEBUG_COLORS=yes NODE_ENV=development node bin/app",
    "dev-install": "cross-env DEBUG_COLORS=yes NODE_ENV=development node bin/install/app",
    "build": "npm run babel",
    "build:watch": "npm run babel:watch",
    "clean": "rimraf lib",
    "babel": "babel src -d lib",
    "babel:watch": "npm run babel -- -w",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env DEBUG_COLORS=yes NODE_ENV=test mocha test --recursive -r @babel/register --verbose --color",
    "pre-commit": "npm run lint && npm run test && npm run build && git add lib"
  },
  "bin": {
    "deps": "bin/app.js",
    "deps-execute": "bin/execute/app.js",
    "deps-install": "bin/install/app.js",
    "deps-deps": "bin/deps.sh",
    "deps-wipe": "bin/wipe.sh",
    "deps-push": "bin/push.sh",
    "deps-head": "bin/head.sh"
  },
  "dependencies": {
    "commander": "^9.4.1",
    "debug": "^4.3.4",
    "glob-all": "^3.3.1",
    "module-alias": "^2.2.2",
    "supports-color": "8.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.3.7",
    "core-js": "^3.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.27.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.1",
    "mocha": "^10.1.0",
    "rimraf": "^3.0.2"
  },
  "_moduleAliases": {
    "@modernpoacher/deps": "./lib",
    "~": "."
  }
}
